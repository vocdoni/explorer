{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "~components/*": [
        "components/*"
      ],
      "~constants": [
        "constants.ts"
      ],
      "~hooks/*": [
        "hooks/*"
      ],
      "~pages/*": [
        "pages/*"
      ],
      "~queries/*": [
        "queries/*"
      ],
      "~utils/*": [
        "utils/*"
      ],
      "~i18n": [
        "i18n/index.ts"
      ],
      "~i18n/*": [
        "i18n/*"
      ],
      "~src/*": [
        "./*"
      ]
    }
  }
}
